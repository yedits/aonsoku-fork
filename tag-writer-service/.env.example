# Port for the tag writer service
PORT=3001

# Navidrome server URL
NAVIDROME_URL=http://localhost:4533

# Navidrome credentials for API access
NAVIDROME_USERNAME=admin
NAVIDROME_PASSWORD=admin

# Music library path (must match Navidrome's music folder)
MUSIC_LIBRARY_PATH=/music

# CORS allowed origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:4533
